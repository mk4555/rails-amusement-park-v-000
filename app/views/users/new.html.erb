<h1>Sign Up</h1>
<%=form_for(@user) do |f|%>
  <%=f.label :name %>
  <%=f.text_field :name%> <br>
  <%=f.label :password%>
  <%=f.text_field :password%> <br>
  <%=f.label :nausea, 'How nauseous are you? (1-5)'%>
  <%=f.text_field :nausea%> <br>
  <%=f.label :happiness, 'How happy are you? (1-5)'%>
  <%=f.text_field :happiness%> <br>
  <%=f.label :tickets%>
  <%=f.text_field :tickets%> <br>
  <%=f.label :height, 'inches'%>
  <%=f.text_field :height%> <br>
  <%=f.label :admin%>
  <%=f.check_box :admin%> <br>
  <%=f.submit 'Create User'%>
<%end%>
